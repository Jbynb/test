<view class="content"><view class="row b-b"><text class="tit">申请项目</text><input class="input" type="text" placeholder="  项目" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','project','$event',[]],['reserveData']]]]]}}" value="{{reserveData.project}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">使用人数</text><input class="input" type="number" placeholder="  使用人数" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','num','$event',[]],['reserveData']]]]]}}" value="{{reserveData.num}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">所属单位</text><input class="input" type="text" placeholder="  所属单位" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','unit','$event',[]],['reserveData']]]]]}}" value="{{reserveData.unit}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="  联系人" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['reserveData']]]]]}}" value="{{reserveData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">联系电话</text><input class="input" type="tel" maxlength="11" placeholder="  联系电话" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['reserveData']]]]]}}" value="{{reserveData.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">项目介绍</text></view><textarea data-event-opts="{{[['input',[['__set_model',['$0','introduce','$event',[]],['reserveData']]]]]}}" class="inputArea" value="{{reserveData.introduce}}" bindinput="__e"></textarea><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><view>{{addressData.lab}}</view></view>